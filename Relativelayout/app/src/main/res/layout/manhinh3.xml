<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/textView1"
        android:layout_centerInParent="true"
        android:paddingTop="10dp"
        android:paddingBottom="10dp"
        android:paddingLeft="40sp"
        android:paddingRight="40sp"
        android:textColor="#000000"
        android:background="#ff2301"
        android:textSize="30sp"
        android:text="1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>
    <TextView
        android:id="@+id/textView2"
        android:layout_above="@+id/textView1"
        android:layout_toLeftOf="@id/textView1"
        android:layout_centerInParent="true"
        android:paddingTop="10dp"
        android:paddingBottom="10dp"
        android:paddingLeft="40sp"
        android:paddingRight="40sp"
        android:textColor="#000000"
        android:background="#2a9545"
        android:textSize="30sp"
        android:text="2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>
    <TextView
        android:id="@+id/textView3"
        android:layout_below="@+id/textView1"
        android:layout_toLeftOf="@id/textView1"
        android:layout_centerInParent="true"
        android:paddingTop="10dp"
        android:paddingBottom="10dp"
        android:paddingLeft="40sp"
        android:paddingRight="40sp"
        android:textColor="#000000"
        android:background="#b7ab33"
        android:textSize="30sp"
        android:text="3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>
    <TextView
        android:id="@+id/textView4"
        android:layout_above="@+id/textView1"
        android:layout_toRightOf="@id/textView1"
        android:layout_centerInParent="true"
        android:paddingTop="10dp"
        android:paddingBottom="10dp"
        android:paddingLeft="40sp"
        android:paddingRight="40sp"
        android:textColor="#000000"
        android:background="#97156f"
        android:textSize="30sp"
        android:text="4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>
    <TextView
        android:id="@+id/textView5"
        android:layout_below="@+id/textView1"
        android:layout_toRightOf="@id/textView1"
        android:layout_centerInParent="true"
        android:paddingTop="10dp"
        android:paddingBottom="10dp"
        android:paddingLeft="40sp"
        android:paddingRight="40sp"
        android:textColor="#000000"
        android:background="#8f625c"
        android:textSize="30sp"
        android:text="5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>
    <TextView
        android:id="@+id/textView6"
        android:paddingTop="10dp"
        android:paddingBottom="10dp"
        android:paddingLeft="40sp"
        android:paddingRight="40sp"
        android:textColor="#000000"
        android:background="#946a30"
        android:textSize="30sp"
        android:text="6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>
    <TextView
        android:id="@+id/textView7"
        android:layout_alignParentRight="true"
        android:paddingTop="10dp"
        android:paddingBottom="10dp"
        android:paddingLeft="40sp"
        android:paddingRight="40sp"
        android:textColor="#000000"
        android:background="#47daae"
        android:textSize="30sp"
        android:text="7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>
    <TextView
        android:id="@+id/textView8"
        android:layout_alignParentBottom="true"
        android:paddingTop="10dp"
        android:paddingBottom="10dp"
        android:paddingLeft="40sp"
        android:paddingRight="40sp"
        android:textColor="#000000"
        android:background="#224d82"
        android:textSize="30sp"
        android:text="8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>
    <TextView
        android:id="@+id/textView9"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:paddingTop="10dp"
        android:paddingBottom="10dp"
        android:paddingLeft="40sp"
        android:paddingRight="40sp"
        android:textColor="#000000"
        android:background="#eb992b"
        android:textSize="30sp"
        android:text="9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>

</RelativeLayout>